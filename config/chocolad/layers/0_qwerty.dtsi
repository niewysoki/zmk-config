/ {
	keymap {
        compatible = "zmk,keymap";
		BASE_LAYER {
			label = "Base";
			bindings = <
            // TOP ROW - keys 0 to 9
                &kp Q       &kp W       &kp E       &kp R       &kp T
                &kp Y       &kp U       &kp I       &kp O       &kp P 

            // MIDDLE ROW - keys 10 to 19
                &hm LGUI A  &hm LALT S  &hm LCTL D  &hm LSHFT F &kp G
                &kp H       &hm LSHFT J &hm LCTL K  &hm LALT L  &hm LGUI SQT 

            // BOTTOM ROW - keys 20 to 29
                &kp Z       &hm RALT X  &kp C       &kp V       &kp B
                &kp N       &kp M       &kp COMMA   &hm RALT DOT &kp FSLH 

            // THUMB ROW - keys 30 to 36
                &lt 1 ESC   &lt 2 SPACE &lt 3 TAB
                &lt 4 RET   &lt 5 BSPC  &lt 6 DEL
			>;
		};	
	};
};